<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Average of Given Enemy Type Appearances in Entire Game</h1>
    <p> Below is a chart that shows the average appearances of each enemy type in the Entire Game.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Average of Given Enemy Type Appearances in Entire Game</legend>
    <table id="print">
      <tr>
       <th>Enemy Type</th>
       <th>Average Appearances in the Game</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Type}}</td>
          <td>{{this.AveInGame}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Count of Enemy Types that Drop Given Item</h1>
    <p> Below is a chart of the count of enemy types that drop a particular item in the game.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Count of Enemy Types that Drop Given Item</legend>
    <table id="print">
      <tr>
       <th>Item Type</th>
       <th>Count of Enemies that Drop this Item</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Type}}</td>
          <td>{{this.EnemyCount}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

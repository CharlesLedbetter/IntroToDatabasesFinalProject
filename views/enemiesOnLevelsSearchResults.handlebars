<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Which Enemy Types are on Which Levels</h1>
    <p>Below are the search results.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Which Enemy Types are on Which Levels</legend>
    <table id="print">
      <tr>
       <th>Stage</th>
       <th>Enemy Type</th>
       <th>Average Number on This Stage</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Stage}}</td>
          <td>{{this.Type}}</td>
          <td>{{this.AveOf}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Special Abilities of Each Enemy Type</h1>
    <p> Below is a chart of special abilities of each enemy type in the game.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Special Abilities of Each Enemy Type</legend>
    <table id="print">
      <tr>
       <th>Enemy Type</th>
       <th>Special Ability</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.EnemyType}}</td>
          <td>{{this.SpecialType}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

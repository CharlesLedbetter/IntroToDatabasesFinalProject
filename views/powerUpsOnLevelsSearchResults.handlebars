<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>How many of Each Power-up are on a Given Level</h1>
    <p>Below are the search results.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Basic Item Data</legend>
    <table id="print">
      <tr>
      <th>Stage</th>
       <th>Power-up Type</th>
       <th>Number on Stage</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Stage}}</td>
          <td>{{this.Type}}</td>
          <td>{{this.NumberOf}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

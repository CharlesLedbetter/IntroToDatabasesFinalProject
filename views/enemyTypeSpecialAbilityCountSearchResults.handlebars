<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Count of Special Abilities per Enemy Type</h1>
    <p>Below are the search results.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Count of Special Abilities per Enemy Type</legend>
    <table id="print">
      <tr>
       <th>Enemy Type</th>
       <th>Number of Special Abilities</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Type}}</td>
          <td>{{this.NumberOfSA}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Basic Enemy Type Data</h1>
    <p> Below is a chart of all basic info regarding the enemy types in the game.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Basic Enemy Type Data</legend>
    <table id="print">
      <tr>
       <th>Enemy Type</th>
       <th>Description</th>
       <th>AttackPoints</th>
       <th>DefensePoints</th>
       <th>Item Dropped Upon Death</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Type}}</td>
          <td>{{this.Description}}</td>
          <td>{{this.AttackPoints}}</td>
          <td>{{this.DefensePoints}}</td>
          <td>{{this.ItemDrop}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>

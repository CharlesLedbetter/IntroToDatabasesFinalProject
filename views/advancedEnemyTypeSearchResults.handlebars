<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <h1>Advanced Enemy Type Data</h1>
    <p>Below are the search results.</p>
  </fieldset>
  <fieldset class="innerFieldset">
    <input type="submit" name="Done" value="Done" id="done" class="wideButton tallButton" onclick="doneButton();">
  </fieldset>
</fieldset>
<fieldset class="formcolor">
  <fieldset class="innerFieldset">
    <legend>Advanced Enemy Type Data</legend>
    <table id="print">
      <tr>
       <th>Enemy Type</th>
       <th>AttackPoints</th>
       <th>DefensePoints</th>
       <th>Item Dropped Upon Death</th>
       <th>Special Abilities</th>
       <th>Stages Enemy Appears On</th>
      </tr>
      {{#each print}}
        <tr>
          <td>{{this.Type}}</td>
          <td>{{this.AttackPoints}}</td>
          <td>{{this.DefensePoints}}</td>
          <td>{{this.Item}}</td>
          <td>{{this.Special}}</td>
          <td>{{this.Stage}}</td>
        </tr>
      {{/each}}
    </table>
  </fieldset>
</fieldset>
